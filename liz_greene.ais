[templates]
# Default template for planet/sign/house combinations when no specific interpretation exists
default_location: {person}'s {planet} in {sign} in the {house} house

# Default template for aspect interpretations when no specific interpretation exists
default_aspect: The {aspect} between {planet1} and {planet2} creates dynamic energy in the chart

# Aspect interpretation templates for common aspects
conjunction: The conjunction of {planet1} and {planet2} creates a powerful fusion of energies
